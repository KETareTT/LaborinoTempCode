<phyphox xmlns="http://phyphox.org/xml" xmlns:editor="http://phyphox.org/editor/xml" version="1.8" editor:version="1.0" locale="de">
    <title>Beschleunigung</title>
    <category>Laborino-Experimente</category>
    <description>Dieses Experiment misst in Echtzeit die Beschleunigung ùëé in x-, y- und z-Richtung und tr√§gt den Betrag sowie die Komponenten aller 3 Raumrichtungen in Graphen √ºber der Zeit auf.</description>
    <icon format="base64">
    </icon>
    <data-containers>
        <!-- COMMUNICATION                                      -->
        <container size="1" init="0" >BT_INC</container>
        <container size="1" init="1" >BT_OUTC</container>

        <!-- DATA                                               -->
        <container size="0" >TIME</container>
        <container size="0" >X</container>
        <container size="0" >Y</container>
        <container size="0" >Z</container>

        <!-- CALCULATIONS                                       -->
        <container size="0" >ABS</container>

        <!-- CALCULATE HELPER                                   -->
        <container size="0" >X_CAL</container>
        <container size="0" >Y_CAL</container>
        <container size="0" >Z_CAL</container>
        <container size="1" init="0">LAST_IND</container>
    </data-containers>
    <input>
        <bluetooth id="Sense" mode="notification" >
            <output char="c0880001-30f7-4671-8b43-5e40ba53514a" conversion="uInt8" offset="0">BT_INC</output>

            <output char="da1a0001-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="0">TIME</output>
            <output char="da1a0001-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="4">X</output>
            <output char="da1a0001-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="8">Y</output>
            <output char="da1a0001-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="12">Z</output>
        </bluetooth>
    </input>
    <output>
        <bluetooth id="Sense" >
            <config char="c0f10001-30f7-4671-8b43-5e40ba53514a" offset="0" conversion="uInt32LittleEndian">4</config>
            <input char="c0880001-30f7-4671-8b43-5e40ba53514a" offset="0" conversion="uInt8">BT_OUTC</input>
        </bluetooth>
    </output>
    <analysis>
        <!-- ================================================== -->
        <!-- SETUPS                                             -->
        <!-- ================================================== -->
        <!-- WHILE THE MEASUREMENT HAS NOT STARTED YET RESET THE DATA BUFFERS SINCE THEY CAN ALSO RECIEVE DATA WITH AN OLD TIMESTAMP AT THE VERY BEGINNING AFTER STARTING THE EXPERIMENT.-->
        <if equal="true">
            <input as="a" keep="true">BT_INC</input>
            <input as="b" type="value">0</input>
            <input as="true" type="empty"></input>
            <output as="result">TIME</output>
        </if>
        <if equal="true">
            <input as="a" keep="true">BT_INC</input>
            <input as="b" type="value">0</input>
            <input as="true" type="empty"></input>
            <output as="result">X</output>
        </if>
        <if equal="true">
            <input as="a" keep="true">BT_INC</input>
            <input as="b" type="value">0</input>
            <input as="true" type="empty"></input>
            <output as="result">Y</output>
        </if>
        <if equal="true">
            <input as="a" keep="true">BT_INC</input>
            <input as="b" type="value">0</input>
            <input as="true" type="empty"></input>
            <output as="result">Z</output>
        </if>

        <!-- ================================================== -->
        <!-- CALCULATIONS                                       -->
        <!-- ================================================== -->
        <!-- COPY NEW DATA INTO CALC BUFFERS                    -->
        <subrange>
            <input as="from">LAST_IND</input>
            <input keep="true">X</input>
            <input keep="true">Y</input>
            <input keep="true">Z</input>
            <output>X_CAL</output>
            <output>Y_CAL</output>
            <output>Z_CAL</output>
        </subrange>

        <!-- SET CURRENT INDEX AS LAST INDEX                    -->
        <count>
            <input keep="true">TIME</input>
            <output>LAST_IND</output>
        </count>

        <!-- CALCULATE THE ABSOLUTE VALUE OF THE ACCELERATION   -->
        <formula formula="sqrt([1_]^2+[2_]^2+[3_]^2)">
            <input keep="true">X_CAL</input>
            <input keep="true">Y_CAL</input>
            <input keep="true">Z_CAL</input>
            <output append="true">ABS</output>
        </formula>
    </analysis>
    <views>
        <view label="Info">
            <info label="Dieses Experiment liest den Beschleunigungssensor des Laborinos in Echtzeit aus. Befindet sich der Laborino in Ruhe oder bewegt sich gleichm√§√üig, so wirkt auf ihn nur die Erdbeschleunigung von etwa 9,81m/s¬≤. Wenn du ihn sch√ºttelst, wirkt auf ihn noch eine weitere Beschleunigung. Wenn du ihn in die Luft wirfst, befindet er sich im freien Fall und es wirkt keine Beschleunigung auf ihn."/>
            <info label=""/>
            <info label="Probiere es doch gleich mal aus!" bold="true"/>
            <info label=""/>
            <info label="Mit dem ‚ñ∫ Knopf oben rechts kannst du die Messung starten. Nach dem Starten der Messung kannst du sie mit dem ‚Ö° Knopf stoppen. Das Papierkorb Symbol l√∂scht den aktuellen Datensatz. Mit dem ‚Åù Symbol kannst du weitere Optionen aufrufen und zum Beispiel die Messung mit einer Verz√∂gerung starten oder die aktuellen Messdaten exportieren."/>
            <info label=""/>
            <info label="Im Tab LIVE werden die aktuellen Messdaten in Echtzeit angezeigt. Mit dem Tab GRAPH kannst du dir alle gemessenen Beschleunigungen √ºber der Zeit auftragen lassen."/>
        </view>
        <view label="Live">
            <info label=""/>
            <info label="Betrag der Beschleunigung:"  bold="true"/>
            <info label="Der Betrag des Beschleunigungsvektors  |ùëé‚Éó| gibt die Gesamtbeschleunigung unabh√§ngig von der Raumrichtung an."/>
            <info label=""/>
            <value label="|ùëé‚Éó| =" unit="m/s¬≤" >
                <input>ABS</input>
            </value>
            <info label=""/>
            <info label="Komponenten der Beschleunigung:"  bold="true"/>
            <info label="Die folgenden 3 Messwerte geben die Beschleunigung ùëé aufgeteilt in die 3 Raumrichtugen (X, Y und Z) wieder. Die Orientierung der Achsen kannst du auf dem Laborino nachlesen."/>
            <info label=""/>
            <image src="image.png" lightFilter="invert" darkFilter="invert"/>
            <info label=""/>
            <value label="ùëé‚Çì    =" unit="m/s¬≤" color="red">
                <input>X</input>
            </value>
            <value label="ùëé·µß    =" unit="m/s¬≤" color="green">
                <input>Y</input>
            </value>
            <value label="ùëé‚ÇÇ    =" unit="m/s¬≤" color="blue">
                <input>Z</input>
            </value>
        </view>
        <view label="Graph">
            <info label="Betrag der Beschleunigung:"  bold="true"/>
            <info label="Der Betrag des Beschleunigungsvektors |ùëé‚Éó| gibt die Gesamtbeschleunigung unabh√§ngig von der Raumrichtung an."/>
            <info label=""/>
            <graph label="" aspectRatio="2.0" style="lines" lineWidth="1.5" color="ffffff" partialUpdate="true" labelX="Zeit ùë°" labelY="Beschleunigung ùëé" unitX="s" unitY="m/s¬≤" xPrecision="3" yPrecision="3">
                <input axis="x">TIME</input>
                <input axis="y">ABS</input>
            </graph>
            <info label=""/>
            <info label="Komponenten der Beschleunigung:"  bold="true"/>
            <info label="Die folgenden 3 Graphen geben die Beschleunigung ùëé aufgeteilt in die 3 Raumrichtugen (X, Y und Z) wieder. Die Orientierung der Richtungen kannst du auf dem Laborino nachlesen."/>
            <info label=""/>
            <image src="image.png" lightFilter="invert" darkFilter="invert"/>
            <info label=""/>
            <graph label="" aspectRatio="2.0" style="lines" lineWidth="1" color="ff7e22" partialUpdate="true" labelX="Zeit ùë°" labelY="Beschleunigung ùëé" unitX="s" unitY="m/s¬≤" xPrecision="3" yPrecision="3">
                <input axis="x"  color="red">TIME</input>
                <input axis="y">X</input>
                <input axis="x" color="green">TIME</input>
                <input axis="y">Y</input>
                <input axis="x"  color="blue">TIME</input>
                <input axis="y">Z</input>
            </graph>
        </view>
    </views>
    <export>
        <set name="Beschleunigung">
            <data name="Zeit t in s">TIME</data>
            <data name="Betrag der Beschleunigung |a| in m/s¬≤">ABS</data>
            <data name="X-Beschleunigung a_x in m/s¬≤">X</data>
            <data name="Y-Beschleunigung a_y in m/s¬≤">Y</data>
            <data name="Z-Beschleunigung a_z in m/s¬≤">Z</data>
        </set>
    </export>
</phyphox>
