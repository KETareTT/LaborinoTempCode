<phyphox xmlns="http://phyphox.org/xml" xmlns:editor="http://phyphox.org/editor/xml" version="1.8" editor:version="1.0" locale="de">
    <title>Zentrifugalbeschleunigung</title>
    <category>Laborino-Experimente</category>
    <description>Das Experiment misst die Zentrifugalbeschleunigung in einem rotierenden Bezugssystem. Dabei orientiert es sich an der Winkelgeschwindigkeit der Y-Achse und der Beschleunigung der Z-Achse.</description>
    <icon format="base64">
    </icon>
    <data-containers>
        <!-- COMMUNICATION                                      -->
        <container size="1" init="0" >BT_INC</container>
        <container size="1" init="1" >BT_OUTC</container>

        <!-- DATA                                               -->
        <container size="0" >TIME</container>
        <container size="0" >ACCZ</container>
        <container size="0" >GYRY</container>

        <!-- CALCULATIONS                                       -->
        <container size="0" >OMEGA_Y</container>
        <container size="0" >OMEGA_Y_POT_TWO</container>

        <!-- CALCULATE HELPER                                   -->
        <container size="0" >GYRY_CAL</container>
        <container size="1" init="0">LAST_IND</container>
    </data-containers>
    <input>
        <bluetooth id="Sense" mode="notification" >
            <output char="c0880001-30f7-4671-8b43-5e40ba53514a" conversion="uInt8" offset="0">BT_INC</output>

            <output char="da1a0002-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="0">TIME</output>
            <output char="da1a0002-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="4">GYRY</output>
            <output char="da1a0002-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="8">ACCZ</output>
        </bluetooth>
    </input>
    <output>
        <bluetooth id="Sense" >
            <config char="c0f10001-30f7-4671-8b43-5e40ba53514a" offset="0" conversion="uInt32LittleEndian">32</config>
            <input char="c0880001-30f7-4671-8b43-5e40ba53514a" offset="0" conversion="uInt8">BT_OUTC</input>
        </bluetooth>
    </output>
    <analysis>
        <!-- ================================================== -->
        <!-- SETUPS                                             -->
        <!-- ================================================== -->
        <!-- WHILE THE MEASUREMENT HAS NOT STARTED YET RESET THE DATA BUFFERS SINCE THEY CAN ALSO RECIEVE DATA WITH AN OLD TIMESTAMP AT THE VERY BEGINNING AFTER STARTING THE EXPERIMENT.-->
        <if equal="true">
            <input as="a" keep="true">BT_INC</input>
            <input as="b" type="value">0</input>
            <input as="true" type="empty"></input>
            <output as="result">TIME</output>
        </if>
        <if equal="true">
            <input as="a" keep="true">BT_INC</input>
            <input as="b" type="value">0</input>
            <input as="true" type="empty"></input>
            <output as="result">ACCZ</output>
        </if>
        <if equal="true">
            <input as="a" keep="true">BT_INC</input>
            <input as="b" type="value">0</input>
            <input as="true" type="empty"></input>
            <output as="result">GYRY</output>
        </if>

        <!-- ================================================== -->
        <!-- CALCULATIONS                                       -->
        <!-- ================================================== -->
        <!-- COPY NEW DATA INTO CALC BUFFERS                    -->
        <subrange>
            <input as="from">LAST_IND</input>
            <input keep="true">GYRY</input>
            <output>GYRY_CAL</output>
        </subrange>

        <!-- SET CURRENT INDEX AS LAST INDEX                    -->
        <count>
            <input keep="true">TIME</input>
            <output>LAST_IND</output>
        </count>

        <!-- CALCULATE THE ANGLE VELOCITIES IN RAD/S; ABOLUTE VALUE OF THE ANGLE VELOCITY AND THE CORRESPONDING FREQUENCIES: -->
        <formula formula="2*3.14159265359*[1_]/360">
            <input keep="true">GYRY_CAL</input>
            <output append="true">OMEGA_Y</output>
        </formula>

        <formula formula="2*3.14159265359*([1_]^2)/360">
            <input keep="true">GYRY_CAL</input>
            <output append="true">OMEGA_Y_POT_TWO</output>
        </formula>
    </analysis>
    <views>
        <view label="Info">
            <info label="Dieses Experiment misst gleichzeitig die Beschleunigung und die Winkelgeschwindigkeit des Laborinos. Aus der Beschleunigung zu verschiedenen Winkelgeschwindigkeiten lässt sich zeigen, dass die Zentrifugalbeschleunigung quadratisch von der Winkelgeschwindigkeit abhängt. Stelle den Laborino dazu aufrecht in eine Schleuder oder Zentrifuge, sodass er sich um die Y-Achse dreht und die Zentrifugalbeschleunigung entlang der Z-Achse wirkt."/>
            <info label=""/>
            <info label="Probiere es doch gleich mal aus!" bold="true"/>
            <info label=""/>
            <info label="Mit dem ► Knopf oben rechts kannst du die Messung starten. Nach dem Starten der Messung kannst du sie mit dem Ⅱ Knopf stoppen. Das Papierkorb Symbol löscht den aktuellen Datensatz. Mit dem ⁝ Symbol kannst du weitere Optionen aufrufen und zum Beispiel die Messung mit einer Verzögerung starten oder die aktuellen Messdaten exportieren."/>
            <info label=""/>
            <info label="Im Tab LIVE werden die die aktuellen Messdaten in Echtzeit angezeigt. Mit dem GRAPH Tab kannst du dir alle gemessenen Beschleunigungen und Winkelgeschwindigkeiten über der Zeit auftragen lassen. In dem Tab ZENTRIFUGALBESCHLEUNIGUNG wird die Beschleunigung über der Winkelgeschwindigkeit aufgetragen, um den quadratischen Zusamenhang von der Winkelgeschwindigkeit sichtbar zu machen."/>
        </view>
        <view label="Zentrifugalbeschleunigung 1">
            <info label="Zentrifugalbeschleunigung:"  bold="true"/>
            <info label="Die folgenden Graphik stellt die gemessene Winkelgeschwindigkeit ω_y auf der X-Achse und die gemessene Beschleunigung a_z auf der Y-Achse dar. Da die Zentrifugalbeschleunigung quadratisch von der Winkelgeschwindigkeit abhängt, bildet sich durch das Drehen der Schleuder bei unterschiedlichen Geschwindigkeiten eine Parabel."/>
            <info label="                                   a_z=r*ω²" bold="true"/>
            <info label=""/>
            <graph label="" aspectRatio="2.0" style="lines" lineWidth="1.5" color="ffffff" partialUpdate="true"  labelX="Winkelgeschwindigkeit ω_y" labelY="Zentrifugalbeschleunigung a_z" unitX="rad/s" unitY="m/s²" xPrecision="3" yPrecision="3">
                <input axis="x">OMEGA_Y</input>
                <input axis="y">ACCZ</input>
            </graph>
        </view>
        <view label="Zentrifugalbeschleunigung 2">
            <info label="Zentrifugalbeschleunigung:"  bold="true"/>
            <info label="Die folgenden Graphik stellt das Quadrat der gemessenen Winkelgeschwindigkeit ω_y auf der X-Achse und die gemessene Beschleunigung a_z auf der Y-Achse dar."/>
            <info label=""/>
            <graph label="" aspectRatio="2.0" style="lines" lineWidth="1.5" color="ffffff" partialUpdate="true"  labelX="Winkelgeschwindigkeit ω²_y" labelY="Zentrifugalbeschleunigung a_z" unitX="rad²/s" unitY="m/s²" xPrecision="3" yPrecision="3">
                <input axis="x">OMEGA_Y_POT_TWO</input> 
                <input axis="y">ACCZ</input>
            </graph>
        </view>
    </views>
    <export>
      <set name="Winkelgeschwindigkeit">
          <data name="Zeit">TIME</data>
          <data name="Y-Winkelgeschwindigkeit in rad/s">OMEGA_Y</data>
          <data name="Quadrat Y-Winkelgeschwindigkeit in rad/s">OMEGA_Y_POT_TWO</data>
          <data name="Z-Beschleunigung in m/s²">ACCZ</data>
      </set>
    </export>
</phyphox>
