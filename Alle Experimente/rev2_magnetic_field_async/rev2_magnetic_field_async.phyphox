<phyphox xmlns="http://phyphox.org/xml" xmlns:editor="http://phyphox.org/editor/xml" version="1.7" editor:version="1.0" locale="">
    <title>Magnetfeld (Async, Rev2)</title>
    <category>Laborino-Experimente</category>
    <description>Dieses Experiment misst die Magnetfeldst√§rke ùêµ in x-, y- und z-Richtung √ºber einen w√§hlbaren Zeitraum mit einstellbarer Frequenz und Messbereich und tr√§gt den Betrag sowie die Komponenten aller 3 Raumrichtungen nach Abschluss der Messung in Graphen √ºber der Zeit auf. Um h√∂here Messfrequenzen als bei der Echtzeitmessung zu erm√∂glichen, werden die Daten erst nach der Messung √ºbertragen.</description>
    <icon format="base64">
    </icon>
    <data-containers>
        <!-- COMMUNICATION                                      -->
        <container size="1" init="0" >BT_INC</container>
        <container size="1" init="1" >BT_OUTC</container>
        <container size="1" init="0">STATUS</container>

        <!-- OTHER                                              -->
        <container size="1" init="1">FS_ID</container>  <!-- FS Doenst work on Rev2, so this just sends a 1, as the setFS function doesnt do anything in Rev2 but is needed as interface -->
        <container size="1" init="1">FS_VAL</container>
        <container size="1" init="7">ODR_ID</container>
        <container size="1" init="30">ODR_VAL</container>
        <container size="1" init="5">PERIOD</container>
        <container size="1" >MAX_PERIOD</container>

        <!-- DATA                                               -->
        <container size="0" >TIME</container>
        <container size="0" >X</container>
        <container size="0" >Y</container>
        <container size="0" >Z</container>

        <!-- CALCULATIONS                                       -->
        <container size="0" >ABS</container>

        <!-- CALCULATE HELPER                                   -->
        <container size="0" >X_CAL</container>
        <container size="0" >Y_CAL</container>
        <container size="0" >Z_CAL</container>
        <container size="1" init="0">LAST_IND</container>
    </data-containers>
    <input>
        <bluetooth id="Sense" mode="notification" >
            <output char="c0880001-30f7-4671-8b43-5e40ba53514a" conversion="uInt8" offset="0">BT_INC</output>
            <output char="c0880001-30f7-4671-8b43-5e40ba53514a" conversion="uInt8" offset="1">STATUS</output>

            <output char="da1a0001-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="0">TIME</output>
            <output char="da1a0001-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="4">X</output>
            <output char="da1a0001-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="8">Y</output>
            <output char="da1a0001-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="12">Z</output>
        </bluetooth>
    </input>
    <output>
        <bluetooth id="Sense" >
            <config char="c0f10001-30f7-4671-8b43-5e40ba53514a" offset="0" conversion="uInt32LittleEndian">9</config>
            <input char="c0880001-30f7-4671-8b43-5e40ba53514a" offset="0" conversion="uInt8">BT_OUTC</input>

            <input char="c0880001-30f7-4671-8b43-5e40ba53514a" offset="1" conversion="uInt8">FS_ID</input>
            <input char="c0880001-30f7-4671-8b43-5e40ba53514a" offset="2" conversion="uInt8">ODR_ID</input>
            <input char="c0880001-30f7-4671-8b43-5e40ba53514a" offset="3" conversion="uInt16LittleEndian">PERIOD</input>
        </bluetooth>
    </output>
    <analysis>
        <!-- ================================================== -->
        <!-- SETUPS                                             -->
        <!-- ================================================== -->
        <!-- WHILE THE MEASUREMENT HAS NOT STARTED YET RESET THE DATA BUFFERS SINCE THEY CAN ALSO RECIEVE DATA WITH AN OLD TIMESTAMP AT THE VERY BEGINNING AFTER STARTING THE EXPERIMENT.-->
        <if equal="true">
            <input as="a" keep="true">BT_INC</input>
            <input as="b" type="value">0</input>
            <input as="true" type="empty"></input>
            <output as="result">TIME</output>
        </if>
        <if equal="true">
            <input as="a" keep="true">BT_INC</input>
            <input as="b" type="value">0</input>
            <input as="true" type="empty"></input>
            <output as="result">X</output>
        </if>
        <if equal="true">
            <input as="a" keep="true">BT_INC</input>
            <input as="b" type="value">0</input>
            <input as="true" type="empty"></input>
            <output as="result">Y</output>
        </if>
        <if equal="true">
            <input as="a" keep="true">BT_INC</input>
            <input as="b" type="value">0</input>
            <input as="true" type="empty"></input>
            <output as="result">Z</output>
        </if>

        <!-- CALCULATE THE MAXIMUM POSSIBLE MEASUREMENT TIME BASED ON THE CHOSEN
        FREQUENCY. WE ASSUMED HERE A MAXIMUM MEASUREMENT LENGTH OF 7200 TIMES 4 (X,Y,Z,TIME)
        DATAPOINTS. OTHERWISE THE ARDUINOS MEMORY IS TOO FULL. -->
        <divide>
            <input as="dividend" type="value">7200</input>
            <input as="divisor" keep="true">ODR_VAL</input>
            <output as="quotient">MAX_PERIOD</output>
        </divide>
        <!-- IF A TOO LARGE VALUE IS ENTERED FOR THE MEASUREMENT TIME, SET IT
        MANUALLY BACK TO THE MAXIMUM MEASUREMENT TIME (MAX_PERIOD). -->
        <if equal="false" greater="true">
            <input as="a" keep="true">PERIOD</input>
            <input as="b" keep="true">MAX_PERIOD</input>
            <input as="true" keep="true">MAX_PERIOD</input>
            <input as="false" keep="true">PERIOD</input>
            <output as="result">PERIOD</output>
        </if>
        <!-- ================================================== -->
        <!-- CALCULATIONS                                       -->
        <!-- ================================================== -->
        <!-- COPY NEW DATA INTO CALC BUFFERS                    -->
        <subrange>
            <input as="from">LAST_IND</input>
            <input keep="true">X</input>
            <input keep="true">Y</input>
            <input keep="true">Z</input>
            <output>X_CAL</output>
            <output>Y_CAL</output>
            <output>Z_CAL</output>
        </subrange>

        <!-- SET CURRENT INDEX AS LAST INDEX                    -->
        <count>
            <input keep="true">TIME</input>
            <output>LAST_IND</output>
        </count>

        <!-- CALCULATE THE ABSOLUTE VALUE OF THE ACCELERATION   -->
        <formula formula="sqrt([1_]^2+[2_]^2+[3_]^2)">
            <input keep="true">X_CAL</input>
            <input keep="true">Y_CAL</input>
            <input keep="true">Z_CAL</input>
            <output append="true">ABS</output>
        </formula>
    </analysis>
    <views>
        <view label="Info">
            <info label="Dieses Experiment liest den Magnetfeldsensor des Laborinos f√ºr einen w√§hlbaren Zeitraum mit einstellbarer Frequenz und Messbereich aus. Die Messdaten werden erst nach dem Ende der Messung √ºbertragen, was h√∂here Messfrequenzen erm√∂glicht als bei der Messung in Echtzeit. Diese hohen Messfrequenzen sind insbesondere bei kurzen aber sehr schnellen Ph√§nomenen von Vorteil, wenn eine hohe Zeitaufl√∂sung n√∂tig ist. Befindet sich der Laborino in der N√§he eines Magneten oder auch nur eines magnetisierten St√ºck Metalls ist die Magnetfeldst√§rke hoch. Entfernst du dich von dem Magneten bzw. Metall sinkt die Magnetfeldst√§rke aber nicht auf null. Dann wird nur noch das Erdmagnetfeld gemessen. Damit kannst zu zum Beispiel bestimmen, wo Norden ist."/>
            <info label=""/>
            <info label="Probiere es doch gleich mal aus!" bold="true"/>
            <info label=""/>
            <info label="Mit dem ‚ñ∫ Knopf oben rechts kannst du die Messung starten. Nach dem Starten der Messung kannst du sie mit dem ‚Ö° Knopf stoppen. Das Papierkorb Symbol l√∂scht den aktuellen Datensatz. Mit dem ‚Åù Symbol kannst du weitere Optionen aufrufen und zum Beispiel die Messung mit einer Verz√∂gerung starten oder die aktuellen Messdaten exportieren."/>
            <info label=""/>
            <info label="In dem GRAPH Tab kannst du dir alle gemessenen Magnetfeldst√§rken √ºber der Zeit auftragen lassen. Der Tab EINSTELLUNGEN erm√∂glicht die Auswahl der Messfrequenz und der Messdauer."/>
        </view>
        <view label="Graph">
          <info label="Status deiner Messung:" bold="true"/>
            <value label="Status:">
                <input>STATUS</input>
                <map max="0">Bereit zum Messen (‚ñ∫)!</map>
                <map max="1">Messung l√§uft...</map>
                <map max="2">Daten√ºbertragung l√§uft...</map>
                <map max="3">Daten√ºbertragung abgeschlossen!</map>
            </value>
            <info label=""/>
            <info label="Betrag des Magnetfeldes:"  bold="true"/>
            <info label="Der Betrag des Magnetfeldvektors |ùêµ‚Éó| gibt die gesamte Magnetfeldst√§rke unabh√§ngig von der Raumrichtung an."/>
            <info label=""/>
            <graph label="" aspectRatio="2.0" style="lines" lineWidth="1.5" color="ffffff"  labelX="Zeit ùë°" labelY="Magnetische Flussdichte ùêµ" unitX="s" unitY="ŒºT" xPrecision="3" yPrecision="3">
                <input axis="x">TIME</input>
                <input axis="y">ABS</input>
            </graph>
            <info label=""/>
            <info label="Komponenten des Magnetfeldes:"  bold="true"/>
            <info label="Die folgenden 3 Graphen geben die Magnetfeldst√§rke ùêµ aufgeteilt in die 3 Raumrichtugen (X, Y und Z) wieder. Die Orientierung der Achsen kannst du auf dem Laborino nachlesen."/>
            <info label=""/>
            <image src="image.png" lightFilter="invert" darkFilter="invert"/>
            <info label=""/>
            <graph label="" aspectRatio="2.0" style="lines" lineWidth="1" color="ff7e22"  labelX="Zeit ùë°" labelY="Magnetische Flussichte ùêµ" unitX="s" unitY="ŒºT" xPrecision="3" yPrecision="3">
                <input axis="x"  color="red">TIME</input>
                <input axis="y">X</input>
                <input axis="x" color="green">TIME</input>
                <input axis="y">Y</input>
                <input axis="x"  color="blue">TIME</input>
                <input axis="y">Z</input>
            </graph>
        </view>
        <view label="Einstellungen">
            <info label="Achtung!" bold="true"  color="red"/>
            <info label="Eine √Ñnderung der Einstellungen ist nur zwischen den Messungen m√∂glich. L√∂sche daf√ºr zun√§chst die aktuellen Messdaten, nehme die √Ñnderungen an den Einstellungen vor und starte dann die Messung neu. Bitte beachte auch, dass nach jedem L√∂schen der Messung die Standardeinstellungen von 30 Hz wiederhergestellt werden." color="red"/>
            <info label=""/>
            <info label="Messbereich:" bold="true"/>
            <info label="Der Messbereich des Magnetfeldsensors (BMM150) in dem Arduino Nano 33 BLE Sense Rev2 ist nicht einstellbar und betr√§gt etwa ¬±1300 ŒºT auf der x- und z-Achse und etwa ¬±2500 ŒºT auf der y-Achse des Laborinos."/>
            <info label=""/>

            <info label="Messfrequenz:" bold="true"/>
            <info label="Hier kannst du die Messfrequenz des Magnetfeldsensors einstellen. Du kannst zwischen 2 Hz, 6 Hz, 8 Hz, 10 Hz, 15 Hz, 20 Hz, 25 Hz und 30 Hz ausw√§hlen."/>
            <info label=""/>
            <value label="Aktuelle Messfrequenz:" precision="0" signed="false" decimal="false"  unit="Hz">
                <input>ODR_VAL</input>
            </value>
            <info label=""/>
            <button label="2 Hz">
                <input type="value">2</input>
                <output>ODR_VAL</output>
                <input type="value">0</input>
                <output>ODR_ID</output>
            </button>
            <button label="6 Hz">
                <input type="value">6</input>
                <output>ODR_VAL</output>
                <input type="value">1</input>
                <output>ODR_ID</output>
            </button>
            <button label="8 Hz">
                <input type="value">8</input>
                <output>ODR_VAL</output>
                <input type="value">2</input>
                <output>ODR_ID</output>
            </button>
            <button label="10 Hz">
                <input type="value">10</input>
                <output>ODR_VAL</output>
                <input type="value">3</input>
                <output>ODR_ID</output>
            </button>
            <button label="15 Hz">
                <input type="value">15</input>
                <output>ODR_VAL</output>
                <input type="value">4</input>
                <output>ODR_ID</output>
            </button>
            <button label="20 Hz">
                <input type="value">20.0</input>
                <output>ODR_VAL</output>
                <input type="value">5</input>
                <output>ODR_ID</output>
            </button>
            <button label="25 Hz">
                <input type="value">25</input>
                <output>ODR_VAL</output>
                <input type="value">6</input>
                <output>ODR_ID</output>
            </button>
            <button label="30 Hz">
                <input type="value">30</input>
                <output>ODR_VAL</output>
                <input type="value">7</input>
                <output>ODR_ID</output>
            </button>
            <info label=""/>
            <info label="Messdauer:" bold="true"/>
            <info label="Hier kannst du die Messdauer einstellen. Aufgrund des beschr√§nkten Speicherplatzes auf dem Laborino, ist die Messdauer in Abh√§ngigkeit der Messfrequenz begrenzt."/>
            <info label=""/>
            <value label="Maximale Messdauer:" precision="0" signed="false" decimal="false" unit="s">
                <input>MAX_PERIOD</input>
            </value>
            <info label=""/>
            <edit label="W√§hle die Messdauer in Sekunden:" precision="0" signed="false" decimal="false" min="1" max="1000"  default="5">
                <output>PERIOD</output>
            </edit>
        </view>
    </views>
    <export>
        <set name="Magnetfeld">
            <data name="Zeit t in s">TIME</data>
            <data name="Betrag der Magnetfeldst√§rke |B| in ŒºT">ABS</data>
            <data name="X-Magnetfeldst√§rke B_x in ŒºT">X</data>
            <data name="Y-Magnetfeldst√§rke B_y in ŒºT">Y</data>
            <data name="Z-Magnetfeldst√§rke B_z in ŒºT">Z</data>
        </set>
    </export>
</phyphox>
